{
  "manifest_version": 3,
  "name": "FocusAssistant",
  "version": "1.0",
  "description": "Focus Assistant",
  "permissions": [
    "storage",
    "identity",
    "alarms",
    "notifications",
    "declarativeNetRequest"
  ],
  "oauth2": {
    "client_id": "113543912380-5rlo3jr2852ajh8fron68r6nqco22igr.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "host_permissions": ["https://www.googleapis.com/*"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  }
